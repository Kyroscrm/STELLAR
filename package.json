  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:health": "jest tests/unit/lib/health.test.ts tests/integration/health/health-endpoint.test.ts",
    "test:health:unit": "jest tests/unit/lib/health.test.ts",
    "test:health:integration": "jest tests/integration/health/health-endpoint.test.ts",
    "analyze": "vite build && source-map-explorer 'dist/assets/*.js'",
    "check-bundle-size": "node scripts/check-bundle-size.js",
    "verify-cache": "node scripts/verify-cache-headers.js",
    "verify-cache:prod": "VERIFY_URL=https://your-production-url.com node scripts/verify-cache-headers.js",
    "verify-cache:staging": "VERIFY_URL=https://staging.your-domain.com node scripts/verify-cache-headers.js",
    "load:smoke": "k6 run --env API_URL=http://localhost:3000 --scenario smoke_test load-tests/k6/script.js",
    "load:peak": "k6 run --env API_URL=http://localhost:3000 --scenario peak_load load-tests/k6/script.js",
    "load:stress": "k6 run --env API_URL=http://localhost:3000 --scenario stress_test load-tests/k6/script.js",
    "load:analyze": "node scripts/analyze-load-test.js",
    "load:report": "npm run load:smoke && npm run load:analyze",
    "monitor:health": "curl -f $HEALTH_CHECK_URL || exit 1",
    "monitor:errors": "node scripts/check-error-rate.js",
    "monitor:latency": "node scripts/check-latency.js",
    "monitor:all": "npm run monitor:health && npm run monitor:errors && npm run monitor:latency"
  },
